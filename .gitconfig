[alias]
  a = add -u
  aa = add
  b = fetch
  br = branch
  c = commit
  co = checkout
  cp = clone
  d = diff
  dh = diff HEAD^
  ds = diff --staged
  f = fetch
  h = help
  l = log
  ol = log --pretty=oneline
  p = push
  rb = rebase
  rbm = rebase master
  s = status -uno
  ss = status
  st = stash
  sta = stash apply
  stl = stash list
[user]
  email = rmnull@users.noreply.github.com
  name = rmnull
[credential]
  helper = cache
[gpg]
  program = gpg2
[init]
  defaultBranch = main
[pull]
  ff = only
[includeIf "gitdir:~/work/"]
  path = ~/work/.gitconfig
